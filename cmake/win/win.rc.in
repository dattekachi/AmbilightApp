IDI_ICON1 ICON DISCARDABLE "${WIN_RC_ICON_PATH}"
1 VERSIONINFO
 FILEVERSION ${AMBILIGHTAPP_VERSION_MAJOR},${AMBILIGHTAPP_VERSION_MINOR},${AMBILIGHTAPP_VERSION_PATCH},0
 PRODUCTVERSION ${AMBILIGHTAPP_VERSION_MAJOR},${AMBILIGHTAPP_VERSION_MINOR},${AMBILIGHTAPP_VERSION_PATCH},0
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "Ambilight App-project"
            VALUE "Comments", "http://ambilightled.com"
            VALUE "FileDescription", "Ambilight App Lighting"
            VALUE "FileVersion", "${AMBILIGHTAPP_VERSION}"
            VALUE "InternalName", "Ambilight App"
            VALUE "LegalCopyright", "MIT"
            VALUE "OriginalFilename", "${BINARY_NAME}"
            VALUE "ProductName", "Ambilight App Lighting"
            VALUE "ProductVersion", "${AMBILIGHTAPP_VERSION}"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
